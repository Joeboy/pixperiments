@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix pg:   <http://ll-plugins.nongnu.org/lv2/ext/portgroups#>.
@prefix atom: <http://lv2plug.in/ns/ext/atom#>.
@prefix urid: <http://lv2plug.in/ns/ext/urid#>.


<http://www.joebutton.co.uk/software/baremetal-music-machine/plugins/piano/out> a pg:StereoGroup.

<http://www.joebutton.co.uk/software/baremetal-music-machine/plugins/piano>
  a lv2:Plugin, lv2:InstrumentPlugin;
  doap:name "Baremetal piano";
  doap:license <http://usefulinc.com/doap/licenses/gpl>;

  lv2:port [
    a atom:AtomPort, lv2:InputPort;
    atom:bufferType atom:Sequence;
    atom:supports <http://lv2plug.in/ns/ext/midi#MidiEvent>,
                  <http://lv2plug.in/ns/ext/patch#Message>;
    lv2:index 0;
    lv2:symbol "control";
    lv2:name "Control in";
  ],

  [
    a lv2:AudioPort, lv2:OutputPort;
    lv2:index 1;
    lv2:symbol "left";
    lv2:name "Left";
    pg:membership [
      pg:group <http://www.joebutton.co.uk/software/baremetal-music-machine/plugins/piano/out>;
      pg:role pg:leftChannel;
    ];
  ],

  [
    a lv2:AudioPort, lv2:OutputPort;
    lv2:index 2;
    lv2:symbol "right";
    lv2:name "Right";
    pg:membership [
      pg:group <http://www.joebutton.co.uk/software/baremetal-music-machine/plugins/piano/out>;
      pg:role pg:rightChannel;
    ];
  ], 

  [
     a lv2:OutputPort, atom:AtomPort ;
     atom:bufferType atom:Sequence ;
     lv2:index 3 ;
     lv2:symbol "notify" ;
     lv2:name "Notify" ;
   ].

